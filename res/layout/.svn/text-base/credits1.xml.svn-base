<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/LinearLayout01"
	android:orientation="vertical"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent">

	<TextView
		android:id="@+id/TextView01"
		android:textSize="30sp"
		android:text="TEAM ZAFKATAPT"
		android:layout_height="wrap_content"
		android:layout_width="fill_parent"
		android:gravity="center" />

	<LinearLayout
		xmlns:android="http://schemas.android.com/apk/res/android"
		android:id="@+id/LinearLayout02"
		android:orientation="horizontal"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content">
		<LinearLayout
			xmlns:android="http://schemas.android.com/apk/res/android"
			android:id="@+id/Sam"
			android:orientation="vertical"
			android:layout_width="wrap_content"
			android:layout_height="fill_parent">
			<TextView
				android:id="@+id/TextView02"
				android:textSize="22sp"
				android:layout_width="fill_parent"
				android:layout_height="wrap_content"
				android:text="Sam Clawson Simons"
				android:gravity="center_horizontal" />
			<TextView
				android:id="@+id/TextView03"
				android:textSize="14sp"
				android:layout_width="fill_parent"
				android:layout_height="wrap_content"
				android:text="Gameplay Design\nGameplay Programmer\nArtist"
				android:gravity="center_horizontal" />
		</LinearLayout>
		<LinearLayout
			xmlns:android="http://schemas.android.com/apk/res/android"
			android:id="@+id/Mango"
			android:orientation="vertical"
			android:layout_width="fill_parent"
			android:layout_height="fill_parent">
			<TextView
				android:id="@+id/TextView04"
				android:textSize="22sp"
				android:layout_width="fill_parent"
				android:layout_height="wrap_content"
				android:text="Michael Tango"
				android:gravity="center_horizontal" />
			<TextView
				android:id="@+id/TextView05"
				android:textSize="14sp"
				android:layout_width="fill_parent"
				android:layout_height="wrap_content"
				android:text="Gameplay Design\nGUI Design\nAndroid Researcher"
				android:gravity="center_horizontal" />
		</LinearLayout>
	</LinearLayout>

	<LinearLayout
		xmlns:android="http://schemas.android.com/apk/res/android"
		android:id="@+id/LinearLayout03"
		android:orientation="horizontal"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content">
		<LinearLayout
			xmlns:android="http://schemas.android.com/apk/res/android"
			android:id="@+id/Roger"
			android:orientation="vertical"
			android:layout_width="wrap_content"
			android:layout_height="fill_parent">
			<TextView
				android:id="@+id/TextView06"
				android:textSize="22sp"
				android:layout_width="fill_parent"
				android:layout_height="wrap_content"
				android:text="Roger Xu"
				android:gravity="center_horizontal" />
			<TextView
				android:id="@+id/TextView07"
				android:textSize="14sp"
				android:layout_width="fill_parent"
				android:layout_height="wrap_content"
				android:text="Networking Design\nNetworking Programmer"
				android:gravity="center_horizontal" />
		</LinearLayout>
		<LinearLayout
			xmlns:android="http://schemas.android.com/apk/res/android"
			android:id="@+id/Zaccardo"
			android:orientation="vertical"
			android:layout_width="fill_parent"
			android:layout_height="fill_parent">
			<TextView
				android:id="@+id/TextView08"
				android:textSize="22sp"
				android:layout_width="fill_parent"
				android:layout_height="wrap_content"
				android:text="Michael Zaccardo"
				android:gravity="center_horizontal" />
			<TextView
				android:id="@+id/TextView09"
				android:textSize="14sp"
				android:layout_width="fill_parent"
				android:layout_height="wrap_content"
				android:text="Gameplay Programmaer\nObject Oriented Design\nAI Programmer"
				android:gravity="center_horizontal" />
		</LinearLayout>
	</LinearLayout>

	<Button
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:onClick="forward1"
		android:text="Next" />


</LinearLayout>
